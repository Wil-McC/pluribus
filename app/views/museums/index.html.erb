<%= link_to "Artifact Index", "/artifacts", method: :get %>
<%= link_to "Museum Index", "/museums", method: :get %>

<h1>All Museums</h1>
<a href="/museums/new">New Museum</a>

<% @museums.each do |museum| %>
  <h3><%= museum.id %> - <a href="/museums/<%= museum.id %>"><%= museum.name %></a> - <%= museum.created_at %></h3>
  <%= link_to "Update #{museum.name}", "/museums/#{museum.id}/edit", method: :get %>
  <%= link_to "Delete #{museum.name}", "/museums/#{museum.id}", method: :delete %>
<% end %>
